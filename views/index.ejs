<% records.forEach( (record, index) => { %>
    <h1>
        <a href="/records/<%= record.id %>">
            <%= record.title %>
        </a>
    </h1>
    <form action='/records/<%= record.id %>?_method=DELETE' method="POST">
        <input type="submit" value="DELETE"/>
    </form>
    <a href='/records/<%= record.id %>/edit'>Edit</a>
<% }) %>
