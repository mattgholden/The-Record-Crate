<% records.forEach( (record, index) => { %>
    <h1>
        <%= record.title %>
    </h1>
    <form action='/records/<%= index %>?_method=DELETE' method="POST">
        <input type="submit" value="DELETE"/>
    </form>
    <a href="/records/<%= index %>/edit">Edit</a>
<% }) %>
